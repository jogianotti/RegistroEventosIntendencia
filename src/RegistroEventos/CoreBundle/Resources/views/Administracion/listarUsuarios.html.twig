<html>
    <head>
        <title></title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <div>
            <table>
                <tr>
                    <td>Usuario</td>
                    <td>Correo</td>
                    <td>Nombre</td>
                    <td>Habilitado</td>
                </tr>
            {% for usuario in usuarios %}
            <tr>
                <td>{{usuario.getUsername()}}</td>
                <td>{{usuario.getEmail()}}</td>
                <td>{{usuario.getNombre()}}</td>
                <td>{{usuario.isEnabled()}}</td>
                <td><a href="{{ path('registro_eventos_core_administracion_usuarios_modificacion')}}?username={{usuario.getUsername()}}">EDITAR</a></td>
            </tr>
            {% endfor %}
            </table>
        </div>
    </body>
</html>
